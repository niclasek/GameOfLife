<table>
    <% $m.times { |i| %>
      <tr>
          <%  $n.times { |j| %>
            <%  if  @grid_hash.cells[i.to_s + " " + j.to_s] == 1  %>
              <td class="alive"> </td>
            <%  else %>
              <td class="dead"> </td>
            <%  end %>
          <% } %>
      </tr>
    <% } %>
</table>
<%= link_to "Next", game_next_path %>